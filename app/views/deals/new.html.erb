<h1> Add a new deal for your organization! </h1>

<%= form_for :deal, url: '/users/:id/deals' do |f| %>
<ul>
  <li> Food Name: <%= f.text_field :food_name %> </li>
  <li> Description: <%= f.text_field :description %> </li>
  <li> Location Of Deal: <%= f.text_field :address %> </li>
  <li> Deal Type: <%= f.text_field :deal_type %> </li>
  <li> Start Time: <%= f.text_field :start_time %> </li>
  <li> End Time: <%= f.text_field :end_time %> </li>
  <li> Food Type: <%= f.text_field :food_type %> </li>
</ul>
  <%= f.submit "Submit" %>
<% end %>
