<h1 class='center'>Login</h1> <br><br>

<div class='center'>
  <%= form_tag '/login' do %>
      <div>
        Email: <%= text_field_tag :email %>
      </div>
      <br>
      <div>
        Password: <%= password_field_tag :password %>
      </div>
      <br>
      <%= submit_tag "Submit" %>

    <% end %>
</div>
